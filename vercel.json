{
    "buildCommand": "cd client ; yarn install ; yarn build",
    "outputDirectory": "client/build",
    "framework": "create-react-app",
    "rewrites": [
        {
            "source": "/server/(.*)",
            "destination": "/server/app.js"
        },
        {
            "source": "/client/(.*)",
            "destination": "/client/build/$1"
        }
    ]
}